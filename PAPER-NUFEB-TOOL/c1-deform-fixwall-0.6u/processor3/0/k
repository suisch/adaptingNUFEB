FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1e-08;

boundaryField
{
    bottom
    {
        type            inletOutlet;
        inletValue      uniform 1e-08;
        value           uniform 1e-08;
    }
    top
    {
        type            fixedValue;
        value           uniform 1e-08;
    }
    left
    {
        type            cyclic;
    }
    right
    {
        type            cyclic;
    }
    front
    {
        type            cyclic;
    }
    back
    {
        type            cyclic;
    }
    procBoundary3to0throughright
    {
        type            processorCyclic;
        value           uniform 1e-08;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 1e-08;
    }
    procBoundary3to2throughfront
    {
        type            processorCyclic;
        value           uniform 1e-08;
    }
}


